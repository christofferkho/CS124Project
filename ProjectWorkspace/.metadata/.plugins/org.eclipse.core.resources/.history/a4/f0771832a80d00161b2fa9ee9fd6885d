package app.receiver.handler;

import java.util.HashMap;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

import app.component.LoginCommand;

@Component
public class LoginRequest extends RequestTemplate {
	
	@Autowired
	private LoginCommand command;
	
	int idNo = -1;
	int terminalPk = -1;

	@Override
	protected void getArgs(HashMap map) {
		// TODO Auto-generated method stub
		String idNoString = (String) map.get(IDNO);
		String terminalPkString = (String) map.get(TERMINALPK);
		
		idNo = Integer.parseInt(idNoString);
		terminalPk = Integer.parseInt(terminalPkString);
	}

	@Override
	protected String doCommand() {
		// TODO Auto-generated method stub
		return null;
	}

}
