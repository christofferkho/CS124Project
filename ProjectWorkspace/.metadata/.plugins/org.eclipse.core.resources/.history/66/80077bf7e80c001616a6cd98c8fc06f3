	private Terminal terminal;

	@Override
	public String undo(Exception e) {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public String execute() {
		// TODO Auto-generated method stub
		return null;
	}
	
	public void createTerminal(int terminalNumber, String roomname){
		terminal = new Terminal(terminalNumber,roomname);
	}

}
