		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public String execute() {
		// TODO Auto-generated method stub
		if(student != null && terminal != null){
			Timestamp timestamp = getDate();
		}
		return null;
	}
	
	public void createAccessLog(int idNo, int terminalPk){
		student = studentDao.findByIdNo(idNo);
		terminal = terminalDao.findById(terminalPk);
	}

}
